        scriptEl.innerHTML = current.innerHTML;
        scriptEl.setAttribute("data-tag", "tracking");
        if (current.getAttribute("data-script-area") === 'head') {
          document.head.appendChild(scriptEl);
        } else {
          document.body.appendChild(scriptEl);
        }
